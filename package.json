{
    "name": "@slimio/json-diff",
    "version": "0.1.1",
    "description": "Stdout difference from two JS Objects in the TTY",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "prepublishOnly": "pkg-ok",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "test": "cross-env psp && ava --verbose",
        "coverage": "nyc npm test",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/json-diff.git"
    },
    "files": [
        "src"
    ],
    "keywords": [
        "json",
        "object",
        "diff",
        "difference",
        "tty",
        "stdout"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/json-diff/issues"
    },
    "homepage": "https://github.com/SlimIO/json-diff#readme",
    "dependencies": {
        "@slimio/is": "^1.5.1",
        "kleur": "^3.0.3",
        "lodash.clonedeep": "^4.5.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.2.0",
        "@commitlint/config-conventional": "^8.2.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^4.0.0",
        "@slimio/psp": "^0.9.2",
        "ava": "^2.4.0",
        "cross-env": "^7.0.2",
        "eslint": "^6.7.0",
        "husky": "^3.1.0",
        "jsdoc": "^3.6.3",
        "nyc": "^14.1.1",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=12"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint index.js src/color.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
